{
  "name": "native-router-react",
  "version": "0.3.0",
  "main": "dist/native-router-react.es.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "start": "vite",
    "build": "vite build && tsc -p tsconfig.production.json && tsc-alias -p tsconfig.production.json",
    "commit": "lint-staged && git-cz -n",
    "coverage": "nyc report --reporter=text-lcov | coveralls",
    "lint": "eslint --fix src test demos *.js --ext .js,.jsx,.ts,.tsx",
    "deploy": "cross-env BABEL_ENV=development BUILD_DEMO=true vite build && gh-pages -d dist",
    "test": "cross-env NODE_ENV=test nyc mocha",
    "test:watch": "npm test -- --watch",
    "prepublishOnly": "npm run build",
    "postpublish": "git push --follow-tags && npm run deploy",
    "serve": "vite preview"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/wmzy/native-router-react"
  },
  "sideEffects": false,
  "author": "wmzy",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/wmzy/native-router-react/issues"
  },
  "homepage": "https://github.com/wmzy/native-router-react",
  "engines": {
    "node": ">=14"
  },
  "peerDependencies": {
    "react": "^17.0.0",
    "react-dom": "^17.0.0"
  },
  "dependencies": {
    "history": "^5.0.1",
    "path-to-regexp": "^6.2.0"
  },
  "devDependencies": {
    "@babel/core": "^7.15.8",
    "@babel/preset-env": "^7.15.8",
    "@babel/preset-react": "^7.14.5",
    "@babel/preset-typescript": "^7.15.0",
    "@babel/register": "^7.15.3",
    "@linaria/babel-preset": "^3.0.0-beta.13",
    "@linaria/core": "^3.0.0-beta.4",
    "@linaria/rollup": "^3.0.0-beta.12",
    "@linaria/shaker": "^3.0.0-beta.12",
    "@testing-library/react": "^12.1.2",
    "@types/mocha": "^9.0.0",
    "@types/node": "^16.7.10",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "@types/sinon": "^10.0.4",
    "@typescript-eslint/eslint-plugin": "^5.1.0",
    "@typescript-eslint/parser": "^5.1.0",
    "@vitejs/plugin-react": "^1.0.1",
    "babel-plugin-module-resolver": "^4.1.0",
    "commitizen": "^4.2.4",
    "core-js": "^3.18.1",
    "coveralls": "^3.1.1",
    "cross-env": "^7.0.3",
    "eslint": "^8.0.1",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-airbnb-typescript": "^14.0.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-import-resolver-typescript": "^2.5.0",
    "eslint-plugin-compat": "^3.13.0",
    "eslint-plugin-import": "^2.25.2",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-mocha": "^9.0.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.26.1",
    "eslint-plugin-react-hooks": "^4.2.0",
    "gh-pages": "^3.2.3",
    "global-jsdom": "^8.3.0",
    "husky": "^7.0.2",
    "jsdom": "^18.0.0",
    "lint-staged": "^11.2.3",
